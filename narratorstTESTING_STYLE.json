{
   "style": [
    {
        "selector":"edge",
        "style":{
            "curve-style": "bezier",
            "opacity": "0.333",
            "width": "2",
            "overlay-opacity": "0",
            "target-arrow-shape": "triangle"
        }
    },
    {
        "selector": "node",
        "style":{
            "width": "40",
            "height": "40",
            "font-size": "9",
            "font-weight": "bold",
            "min-zoomed-font-size": "4",
            "text-wrap": "wrap",
            "text-max-width": "50",
            "text-valign": "center",
            "text-halign": "center",
            "text-events": "yes",
            "color": "#000",
            "text-outline-width": "1",
            "text-outline-color": "#fff",
            "text-outline-opacity": "1",
            "overlay-color": "#fff"
        }
    },
    {
        "selector": "node[gender='male']",
        "style": {
            "background-color":"#99cc00",
            "text-outline-color": "#99cc00"
        }
    },
    {
        "selector": "node[gender='female']",
        "style": {
            "background-color": "#cc99ff",
            "text-outline-color": "#cc99ff"
        }
    }
    ]
}